    "n = input('Enter the value for the Fibonacci sequence: ')\n",
    "def fib(n):\n",
    "   # Print the Fibonacci series up to n\n",
    "    result = []\n",
    "    a,b = 0,1\n",
    "    while a < n:\n",
    "        result.append(a)\n",
    "        a, b = b, a+b\n",
    "    return result\n",
    "print fib(n)"
